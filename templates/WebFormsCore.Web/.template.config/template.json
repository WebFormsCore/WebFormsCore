{
  "$schema": "http://json.schemastore.org/template",
  "author": "WebFormsCore",
  "classifications": ["Web", "ASP.NET", "WebForms"],
  "identity": "WebFormsCore.Templates.Web",
  "name": "WebForms Core Web Application",
  "shortName": "wfc",
  "sourceName": "WebFormsCore.Web",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "WebFormsCoreVersion": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "$version$",
      "replaces": "{WebFormsCoreVersion}",
      "description": "The version of the WebFormsCore SDK to use."
    }
  }
}
